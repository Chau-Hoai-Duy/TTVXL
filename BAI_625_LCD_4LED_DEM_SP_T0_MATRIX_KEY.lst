....................            case 1:
....................                  if(input(oldpin))step = 0;
....................                  else if((pin==oldpin)&&(delay_stop(t)>=30))
....................                    {
....................                       t    = delay_start();
....................                       step = 2;
....................                       return OK;
....................                    }
....................                  break;
....................             case 2:
....................                  if(input(oldpin))step = 0;
